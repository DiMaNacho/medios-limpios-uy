{
  "background": {
    "service_worker": "./background.js"
  },
  "description": "Quitando todo lo que complica la lectura de notas en distintos portales de noticias uruguayos.",
  "host_permissions": [
    "*://www.elobservador.com.uy/*",
    "*://elobservador.com.uy/*",
    "*://www.elpais.com.uy/*",
    "*://elpais.com.uy/*"
  ],
  "icons": {
    "16": "./icons/logo16x16.png",
    "32": "./icons/logo32x32.png",
    "48": "./icons/logo48x48.png",
    "64": "./icons/logo64x64.png",
    "128": "./icons/logo128x128.png"
  },
  "manifest_version": 3,
  "name": "Medios Limpios UY",
  "permissions": ["tabs", "scripting", "http://*/*", "https://*/*", "storage"],
  "version": "1.0.60",
  "web_accessible_resources": [
    {
      "resources": [
        "./js/elobservador.js",
        "./js/elpais.js",
        "./styles/elobservador.css",
        "./styles/elpais.css"
      ],
      "matches": [
        "*://www.elobservador.com.uy/*",
        "*://elobservador.com.uy/*",
        "*://www.elpais.com.uy/*",
        "*://elpais.com.uy/*"
      ]
    }
  ]
}
